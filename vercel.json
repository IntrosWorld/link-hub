{
  "buildCommand": "cd client && npm install && npm run build && cd .. && cp -r client/dist public",
  "installCommand": "npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "/api/index.ts"
    },
    {
      "source": "/go/:id",
      "destination": "/api/index.ts"
    }
  ]
}
